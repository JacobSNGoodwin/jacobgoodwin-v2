---
export type Props = {
  metaTags?: MetaTags;
};

import { baseMetaTags } from "@data/baseMetaTags";
import { MetaTags } from "src/types";
import SectionLayout from "./SectionLayout.astro";


const { metaTags } = Astro.props as Props;

const tags: MetaTags = {
  ...baseMetaTags,
  ...metaTags,
}
---

<SectionLayout navItems={[]} logoPath="/test" metaTags={tags}>
  <slot />
</SectionLayout>